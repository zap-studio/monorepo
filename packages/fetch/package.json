{
	"name": "@zap-studio/fetch",
	"version": "0.1.1",
	"type": "module",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"build": "tsdown --config tsdown.config.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"zod": "^4.1.12"
	},
	"devDependencies": {
