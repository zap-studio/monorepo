{
	"name": "@zap-studio/webhooks",
	"version": "0.0.0",
	"type": "module",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"build": "tsdown --config tsdown.config.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"peerDependencies": {
		"arktype": ">=2.0.0",
		"valibot": ">=0.30.0",
		"yup": ">=1.0.0",
		"zod": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"arktype": {
			"optional": true
		},
		"valibot": {
			"optional": true
		},
		"yup": {
			"optional": true
		},
		"zod": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/node": "latest",
		"@zap-studio/tsdown-config": "workspace:*",
		"@zap-studio/typescript-config": "workspace:*",
		"@zap-studio/vitest-config": "workspace:*",
		"jsdom": "latest",
		"tsdown": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"exports": {
		".": "./dist/index.js",
		"./adapters": "./dist/adapters/index.js",
		"./types": "./dist/types/index.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
