{
	"name": "@zap-studio/waitlist",
	"version": "0.0.0",
	"type": "module",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown --config tsdown.config.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"nanoid": "^5.1.6",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@zap-studio/tsdown-config": "workspace:*",
		"@zap-studio/typescript-config": "workspace:*",
		"@zap-studio/vitest-config": "workspace:*",
		"jsdom": "latest",
		"tsdown": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"exports": {
		"./adapters/in-memory": {
			"import": "./dist/adapters/in-memory.js",
			"require": "./dist/adapters/in-memory.cjs"
		},
		"./client": {
			"import": "./dist/client.js",
			"require": "./dist/client.cjs"
		},
		"./core/schemas": {
			"import": "./dist/core/schemas.js",
			"require": "./dist/core/schemas.cjs"
		},
		"./core/utils": {
			"import": "./dist/core/utils.js",
			"require": "./dist/core/utils.cjs"
		},
		"./core/validators": {
			"import": "./dist/core/validators.js",
			"require": "./dist/core/validators.cjs"
		},
		"./events": {
			"import": "./dist/events/index.js",
			"require": "./dist/events/index.cjs"
		},
		"./referral": {
			"import": "./dist/referral/index.js",
			"require": "./dist/referral/index.cjs"
		},
		"./sdk/base": {
			"import": "./dist/sdk/base.js",
			"require": "./dist/sdk/base.cjs"
		},
		"./sdk/types": {
			"import": "./dist/sdk/types.js",
			"require": "./dist/sdk/types.cjs"
		},
		"./server": {
			"import": "./dist/server.js",
			"require": "./dist/server.cjs"
		},
		"./types": {
			"import": "./dist/types/index.js",
			"require": "./dist/types/index.cjs"
		},
		"./package.json": "./package.json"
	}
}
