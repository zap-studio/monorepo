{
	"name": "@zap-studio/waitlist-adapters",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsdown --config tsdown.config.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"@zap-studio/waitlist-core": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@zap-studio/typescript-config": "workspace:*",
		"@zap-studio/tsdown-config": "workspace:*",
		"@zap-studio/vitest-config": "workspace:*",
		"jsdom": "latest",
		"typescript": "latest",
		"tsdown": "latest",
		"vitest": "latest"
	},
	"exports": {
		"./in-memory": {
			"import": "./dist/in-memory.js",
			"require": "./dist/in-memory.cjs"
		},
		"./types": {
			"import": "./dist/types.js",
			"require": "./dist/types.cjs"
		},
		"./package.json": "./package.json"
	}
}
