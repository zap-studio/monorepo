{
  "name": "@zap-ts/architecture",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": false,
  "version": "1.0.3",
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "clean": "rm -rf ./dist",
    "postbuild": "typedoc --entryPointStrategy expand src"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "typedoc": "^0.28.11"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "react": "^19.2.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./deps": {
      "import": "./dist/deps.js",
      "require": "./dist/deps.cjs"
    },
    "./files": {
      "import": "./dist/files/index.js",
      "require": "./dist/files/index.cjs"
    },
    "./helpers/link": {
      "import": "./dist/helpers/link.js",
      "require": "./dist/helpers/link.cjs"
    },
    "./ide": {
      "import": "./dist/ide.js",
      "require": "./dist/ide.cjs"
    },
    "./plugins": {
      "import": "./dist/plugins.js",
      "require": "./dist/plugins.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./package.json": "./package.json"
  }
}
